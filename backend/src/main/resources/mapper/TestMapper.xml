<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.finalpj.backend.service.TestMapper">
  <insert id="regist" parameterType="UserVO">
  		insert into user(user_id,
  							user_pw,
  							user_name,
  							user_birth,
  							user_email,
  							user_phone,
  							user_gender,
  							user_postcode,
  							user_address,
  							user_detail_address
  							)
  		values( #{user_id},
  				#{user_pw},
  				#{user_name},
  				#{user_birth},
  				#{user_email},
  				#{user_phone},
  				#{user_gender},
  				#{user_postcode},
  				#{user_address},
  				#{user_detail_address}
  				)
  	</insert>

	<select id="checkId" resultType="int">
		select count(*) from user where user_id=#{user_id}
	</select>

